{
  "name": "vite-react-firebase",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "firebase emulators:exec --only auth,firestore 'npm run dev:vite'",
    "dev:vite": "vite",
    "dev:firebase": "firebase emulators:start --only auth,firestore",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "kill:firebase": "lsof -t -i :9099 -i:5001 -i:8090 -i:4000 -i:8080 -i:8085 | xargs kill -9"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.28.0",
    "firebase": "^10.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
